<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.wxportal.dao.bean">

    <class name="RespMusicBean" table="resp_music">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="reqKey" type="java.lang.String" column="reqkey"/>
        <property name="title" type="java.lang.String" column="tilte"/>
        <property name="description" type="java.lang.String" column="description"/>
        <property name="linkUrl" type="java.lang.String" column="linkurl"/>
        <property name="realUrl" type="java.lang.String" column="realurl"/>
        <property name="wxAccountId" type="java.lang.Integer" column="wxaccountid"/>
    </class>

</hibernate-mapping>
